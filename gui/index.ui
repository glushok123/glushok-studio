}

QSpinBox, QDoubleSpinBox, QLineEdit {
    background: rgba(11, 21, 38, 0.92);
    border: 1px solid rgba(122, 215, 255, 0.45);
    border-radius: 10px;
    padding: 6px 10px;
    color: #f4f7ff;
}

QSpinBox:focus, QDoubleSpinBox:focus, QLineEdit:focus {
    border-color: #7ad7ff;
    box-shadow: 0 0 12px rgba(122, 215, 255, 0.25);
}

QCheckBox {
    spacing: 8px;
    font-weight: 500;
    color: #c7d7ff;
}

QCheckBox::indicator {
    width: 18px;
    height: 18px;
    border-radius: 5px;
    border: 2px solid rgba(122, 215, 255, 0.6);
    background: rgba(8, 15, 30, 0.9);
}

QCheckBox::indicator:checked {
    background-color: #7ad7ff;
    border-color: #7ad7ff;
}

QPushButton {
    background-color: #1c2f52;
    border-radius: 12px;
    border: 2px solid rgba(122, 215, 255, 0.55);
    padding: 10px 24px;
    font-weight: 600;
    color: #f4f7ff;
    text-transform: uppercase;
}

QPushButton:hover {
    background-color: #243a63;
    border-color: #7ad7ff;
    color: #7ad7ff;
}

QPushButton:pressed {
    background-color: #152745;
}

QProgressBar {
    background: rgba(11, 21, 38, 0.92);
    border-radius: 12px;
    border: 1px solid rgba(122, 215, 255, 0.35);
    height: 32px;
    color: #0d1628;
    font-weight: 600;
}

QProgressBar::chunk {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #7ad7ff, stop:1 #8d9eff);
    border-radius: 10px;
}

QTabWidget::pane {
    border: 1px solid rgba(122, 215, 255, 0.35);
    border-radius: 12px;
    background: rgba(11, 21, 38, 0.65);
}

QTabBar::tab {
    background: rgba(11, 21, 38, 0.85);
    padding: 8px 22px;
    color: #c7d7ff;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border: 1px solid transparent;
    margin-right: 4px;
}

QTabBar::tab:selected {
    background: rgba(122, 215, 255, 0.2);
    color: #7ad7ff;
    border-color: rgba(122, 215, 255, 0.45);
}

QListWidget {
    background: rgba(11, 21, 38, 0.9);
    border: 1px solid rgba(122, 215, 255, 0.35);
    border-radius: 12px;
    padding: 8px;
}

QTextEdit {
    background: rgba(11, 21, 38, 0.9);
    border: 1px solid rgba(122, 215, 255, 0.35);
    border-radius: 12px;
    padding: 10px;
    color: #f4f7ff;
}

QScrollBar:vertical {
    background: rgba(11, 21, 38, 0.8);
    width: 12px;
    margin: 4px;
    border-radius: 6px;
}

QScrollBar::handle:vertical {
    background: #7ad7ff;
    border-radius: 6px;
}

QMenuBar {
    background: transparent;
    color: #c7d7ff;
}

QMenuBar::item:selected {
    background: rgba(122, 215, 255, 0.2);
}

QStatusBar {
    background: transparent;
    color: #7ad7ff;
}
